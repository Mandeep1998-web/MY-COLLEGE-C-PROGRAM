#include<stdio.h>
#include<conio.h>
int main()
{
int arr[] = { 1, 2, 2, 2, 2, 3, 4, 7, 8,8,8};
int n = sizeof(arr) / sizeof(int);
int start=-1 , end=-1;
int target =8;
int i;
for(i=0;i<11;i++){
    if(target!=arr[i])
        continue;
    if(start==-1){
        start=i;
    }
    else
    {

        end=i;
    }
}
    if(start!=-1)
    {
        printf("%d\n",start);
        printf("%d",end);
    }
}

